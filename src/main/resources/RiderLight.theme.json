{
  "name": "Rider Light (plugin)",
  "dark": false,
  "author": "JetBrains",
  "editorScheme": "/RiderLight.xml",
  "ui": {
    "*": {
      "//*": "#FF00FF",
      "//background": "#FF0000",
      "//foreground": "#00FF00",
      "//borderColor": "#FF00FF",
      "selectionBackground": "$selectionBackground",
      "selectionForeground": "$selectionForeground",
      "selectionInactiveBackground": "$selectionBackgroundInactive",
      "selectionBackgroundInactive": "$selectionBackgroundInactive",
      "selectionInactiveForeground": "$selectionForegroundInactive",
      "selectionForegroundInactive": "$selectionForegroundInactive"
    },

    "Panel": {
      "background": "$panelBackground",
      "foreground": "$regularForeground"
    },

    "Borders": {
      "color": "$border",
      "ContrastBorderColor": "$border"
    },

    "NavBar": {
      "borderColor": "$border"
    },

    "StatusBar": {
      "borderColor": "$border"
    },

    "SearchEverywhere": {
      "Header.background": "$panelBackground",
      "Tab": {
        "selectedBackground": "$textInputBackground",
        "selectedForeground": "$selectionForeground"
      },
      "SearchField": {
        "background": "$textInputBackground",
        "borderColor": "$textInputBackground",
        "infoForeground": "$disabledForeground"
      },
      "List": {
        "separatorForeground": "$mutedForeground",
        "separatorColor": "$border"
      },
      "Advertiser": {
        "background": "$panelBackground",
        "borderColor": "$panelBackground",
        "foreground": "$mutedForeground"
      }
    },

    "SearchMatch": {
      "startBackground": "#e6bd1b",
      "endBackground": "#e6bd1b"
    },

    "SpeedSearch": {
      "background": "#f7f0d2",
      "borderColor": "#FFFFFF80",
      "foreground": "$regularForeground",
      "errorForeground": "#d91400"
    },

    "DefaultTabs": {
      "background": "$panelBackground",
      "hoverBackground": "$textInputBackground",
      "underlineColor": "$selectionBackground",
      "inactiveUnderlineColor": "$selectionBackgroundInactive",
      "underlinedTabBackground": "$textInputBackground"
    },

    "EditorTabs": {
      "borderColor": "$border",
      "background": "$panelBackground",
      "underlineColor": "#5397E6",
      "selectedBackground": "$textInputBackground",
      "underlinedTabBackground": "$textInputBackground"
    },

    "ToolWindow": {
      "Button": {
        "selectedBackground": "#D0D0D0",
        "selectedForeground": "$regularForeground",
        "hoverBackground": "#F8F8F8"
      },
      "Header": {
        "borderColor": "#CBCBCB",
        "background": "$panelBackground",
        "inactiveBackground": "#E6E6E6"
      },
      "HeaderTab": {
        "selectedBackground": "#d0d0d0",
        "selectedInactiveBackground": "#D4D4D4",
        "hoverBackground": "#F8F8F8",
        "hoverInactiveBackground": "#F8F8F8"
      }
    },

    "ScrollBar": {
      "background": "$panelBackground",

      "trackColor": "$panelBackground",
      "Mac.trackColor": "$panelBackground",
      "hoverTrackColor": "$panelBackground",
      "Mac.hoverTrackColor": "$panelBackground",

      "Transparent.trackColor": "#E8E8E800",
      "Mac.Transparent.trackColor": "#E8E8E800",
      "Transparent.hoverTrackColor": "#E8E8E800",
      "Mac.Transparent.hoverTrackColor": "#E8E8E800",

      "thumbBorderColor": "#88889940",
      "Mac.thumbBorderColor": "#88889940",
      "hoverThumbBorderColor": "#888899",
      "Mac.hoverThumbBorderColor": "#888899",

      "thumbColor": "#88889940",
      "Mac.thumbColor": "#88889940",
      "hoverThumbColor": "#888899",
      "Mac.hoverThumbColor": "#888899",

      "Transparent.thumbBorderColor": "#88889940",
      "Mac.Transparent.thumbBorderColor": "#88889940",
      "Transparent.hoverThumbBorderColor": "#888899",
      "Mac.Transparent.hoverThumbBorderColor": "#888899",

      "Transparent.thumbColor": "#88889940",
      "Mac.Transparent.thumbColor": "#88889940",
      "Transparent.hoverThumbColor": "#888899",
      "Mac.Transparent.hoverThumbColor": "#888899"
    },

    "CheckBox": {
      "background": "$panelBackground",
      "foreground": "$regularForeground",
      "disabledText": "$disabledForeground"
    },

    "RadioButton": {
      "background": "$panelBackground",
      "foreground": "$regularForeground",
      "disabledText": "$disabledForeground"
    },

    "Button": {
      "background": "$panelBackground",
      "foreground": "$regularForeground",
      "startBorderColor": "$controlBorder",
      "endBorderColor": "$controlBorder",
      "focusedBorderColor": "$focusedBorder",
      "startBackground": "$buttonFace",
      "endBackground": "$buttonFace",
      "shadowWidth": 0,
      "default": {
        "foreground": "$selectionForeground",
        "startBorderColor": "$focusedBorder",
        "endBorderColor": "$focusedBorder",
        "focusedBorderColor": "$focusedBorder",
        "startBackground": "$selectionBackground",
        "endBackground": "$selectionBackground"
      }
    },

    "ToggleButton": {
      "background": "#FF00FF",
      "borderColor": "$controlBorder",
      "foreground": "$regularForeground",
      "buttonColor": "$controlBorder",
      "onBackground": "$selectionBackground",
      "onForeground": "$selectionForeground",
      "offBackground": "$buttonFace",
      "offForeground": "$mutedForeground",
      "disabledText": "$disabledForeground"
    },

    "List": {
      "background": "$toolwindowBackground",
      "foreground": "$regularForeground",
      "selectionBackground": "$selectionBackground",
      "selectionForeground": "$selectionForeground"
    },

    "Spinner": {
      "background": "$panelBackground"
    },

    "HelpTooltip": {
      "background": "$panelBackground",
      "borderColor": "$border",
      "foreground": "$regularForeground"
    },

    "ParameterInfo": {
      "background": "$panelBackground",
      "currentOverloadBackground": "$selectionBackground",
      "borderColor": "$popupBorder",
      "foreground": "$regularForeground",
      "currentParameterForeground": "$selectionForeground",
      "disabledForeground": "$disabledForeground",
      "infoForeground": "$regularForeground",
      "lineSeparatorColor": "$panelBackground"
    },

    "Menu": {
      "background": "$panelBackground",
      "borderColor": "$popupBorder",
      "foreground": "$regularForeground",
      "selectionForeground": "$selectionForeground",
      "separatorColor": "$separator",
      "acceleratorForeground": "$mutedForeground",
      "acceleratorSelectionForeground": "$selectionForeground",
      "disabledForeground": "$disabledForeground"
    },

    "PopupMenu": {
      "borderWidth": 1,
      "borderInsets": "4,1,4,1",
      "background": "$panelBackground",
      "translucentBackground": "$panelBackground",
      "foreground": "$regularForeground"
    },

    "Popup": {
      "paintBorder": true,
      "borderColor": "$popupBorder",
      "inactiveBorderColor": "$popupBorder",
      "separatorColor": "$separator",
      "separatorForeground": "$mutedForeground",
      "Header": {
        "activeBackground": "$panelBackground",
        "inactiveBackground": "$panelBackground"
      },
      "Toolbar": {
        "background": "$panelBackground",
        "borderColor": "#F8F8F8"
      },
      "Advertiser": {
        "background": "$panelBackground",
        "borderColor": "$panelBackground",
        "foreground": "$mutedForeground"
      }
    },

    "MenuBar": {
      "background": "$panelBackground",
      "foreground": "$regularForeground",
      "borderColor": "$panelBackground",
      "disabledBackground": "$panelBackground",
      "disabledForeground": "$disabledForeground"
    },

    "MenuItem": {
      "background": "$panelBackground",
      "foreground": "$regularForeground",
      "selectionBackground": "$selectionBackground",
      "selectionForeground": "$selectionForeground",
      "acceleratorForeground": "$mutedForeground",
      "acceleratorSelectionForeground": "$selectionForeground",
      "disabledForeground": "$disabledForeground"
    },

    "CheckBoxMenuItem": {
      "background": "$panelBackground",
      "foreground": "$regularForeground",
      "selectionBackground": "$selectionBackground",
      "selectionForeground": "$selectionForeground",
      "acceleratorForeground": "$mutedForeground",
      "acceleratorSelectionForeground": "$selectionForeground",
      "disabledForeground": "$disabledForeground"
    },

    "RadioButtonMenuItem": {
      "background": "$panelBackground",
      "foreground": "$regularForeground",
      "selectionBackground": "$selectionBackground",
      "selectionForeground": "$selectionForeground",
      "acceleratorForeground": "$mutedForeground",
      "acceleratorSelectionForeground": "$selectionForeground",
      "disabledForeground": "$disabledForeground"
    },

    "CompletionPopup": {
      "background": "$toolwindowBackground",
      "foreground": "$regularForeground",
      "infoForeground": "$mutedForeground",
      "matchForeground": "#9C5800",
      "matchSelectionForeground": "#9C5800"
    },

    "Label": {
      "background": "$panelBackground",
      "foreground": "$regularForeground",
      "disabledForeground": "$disabledForeground",
      "selectedDisabledForeground": "$disabledForeground"
    },

    "ComboBox": {
      "foreground": "$regularForeground",
      "background": "$buttonFace",
      "disabledForeground": "$disabledForeground",
      "nonEditableBackground": "$buttonFace",
      "ArrowButton.background": "$buttonFace",
      "ArrowButton.nonEditableBackground": "$buttonFace",
      "ArrowButton.iconColor": "#82828f",
      "ArrowButton.disabledIconColor": "82828f66"
    },

    "ComboBoxButton": {
      "background": "$buttonFace"
    },

    "ComboPopup.border": "1,1,1,1,D0D0D0",

    "Tree": {
      "background": "$toolwindowBackground",
      "foreground": "$regularForeground"
    },

    "Notification": {
      "background": "$notificationBackground",
      "borderColor": "$notificationBorder",
      "foreground": "$notificationForeground",

      "errorBackground": "$validationErrorBackground",
      "errorBorderColor": "$validationErrorBorder",
      "errorForeground": "$validationErrorForeground",

      "ToolWindow": {
        "errorBackground": "$validationErrorBackground",
        "errorBorderColor": "$validationErrorBorder",
        "errorForeground": "$validationErrorForeground",

        "warningBackground": "$validationWarningBackground",
        "warningBorderColor": "$validationWarningBorder",
        "warningForeground": "$validationWarningForeground",

        "informativeBackground": "$validationInformativeBackground",
        "informativeBorderColor": "$validationInformativeBorder",
        "informativeForeground": "$validationInformativeForeground"
      },

      "MoreButton": {
        "background": "$notificationBackground",
        "innerBorderColor": "$notificationBackground",
        "foreground": "$linkRegular"
      }
    },

    "ValidationTooltip": {
      "errorBackground": "$validationErrorBackground",
      "errorBorderColor": "$validationErrorBorder",
      "warningBackground": "$validationWarningBackground",
      "warningBorderColor": "$validationWarningBorder"
    },

    "Link": {
      "activeForeground": "$linkRegular",
      "hoverForeground": "$linkAccented",
      "pressedForeground": "$linkRegular",
      "visitedForeground": "$linkRegular"
    },

    "Plugins": {
      "background": "$toolwindowBackground",
      "lightSelectionBackground": "$selectionBackground",
      "SectionHeader": {
        "background": "$panelBackground",
        "foreground": "$regularForeground"
      },
      "Tab": {
        "selectedBackground": "#FFFFFF",
        "selectedForeground": "$regularForeground",
        "hoverBackground": "#F8F8F8"
      },
      "SearchField": {
        "background": "$textInputBackground",
        "borderColor": "$panelBackground"
      },
      "Button": {
        "installFillBackground": "#FFFFFF",
        "installFillForeground": "$regularForeground",
        "installBackground": "#FFFFFF",
        "installBorderColor": "#BFBFBF",
        "installForeground": "$regularForeground",
        "updateBackground": "#FFFFFF",
        "updateBorderColor": "#BFBFBF",
        "updateForeground": "$regularForeground"
      },
      "tagBackground": "$panelBackground",
      "tagForeground": "$regularForeground"
    },

    "Component": {
      "borderColor": "$controlBorder",
      "focusedBorderColor": "$focusedBorder",
      "disabledBorderColor": "$controlBorder",
      "errorFocusColor": "#DB5860A0",
      "inactiveErrorFocusColor": "#DB586060",
      "warningFocusColor": "#DE971FA0",
      "inactiveWarningFocusColor": "#DE971F60",
      "iconColor": "$mutedForeground",
      "hoverIconColor": "$mutedForeground"
    },

    "ToolTip": {
      "background": "$toolwindowBackground",
      "foreground": "$regularForeground",
      "infoForeground": "$regularForeground",
      "Actions.background": "$panelBackground",
      "Actions.infoForeground": "$regularForeground"
    },

    "TextField": {
      "background": "$textInputBackground",
      "foreground": "$regularForeground",
      "inactiveBackground": "$textInputBackground",
      "inactiveForeground": "$regularForeground"
    },

    "FormattedTextField": {
      "background": "$textInputBackground",
      "foreground": "$regularForeground",
      "inactiveBackground": "$textInputBackground",
      "inactiveForeground": "$regularForeground"
    },

    "PasswordField": {
      "background": "$textInputBackground",
      "foreground": "$regularForeground"
    },

    "TextArea": {
      "background": "$textInputBackground",
      "foreground": "$regularForeground",
      "inactiveBackground": "$textInputBackground",
      "inactiveForeground": "$regularForeground"
    },

    "ActionButton": {
      "hoverBackground": "$buttonFace",
      "hoverBorderColor": "$buttonFace",
      "pressedBackground": "$buttonFace",
      "pressedBorderColor": "$controlBorder"
    },

    "Table": {
      "background": "$toolwindowBackground",
      "foreground": "$regularForeground",
      "stripeColor": "$textInputBackground",
      "gridColor": "$panelBackground",
      "sortIconColor": "$mutedForeground",
      "lightSelectionBackground": "$selectionBackground",
      "lightSelectionForeground": "$selectionForeground",
      "lightSelectionInactiveBackground": "$selectionBackgroundInactive",
      "lightSelectionInactiveForeground": "$selectionForeground"
    },

    "TableHeader": {
      "background": "$panelBackground",
      "foreground": "$mutedForeground",
      "cellBorder": "1,5,2,5"
    },

    "ProgressBar": {
      "trackColor": "#FFFFFF",
      "progressColor": "#5a5a66B0",
      "indeterminateStartColor": "#5a5a66B0",
      "indeterminateEndColor": "#5a5a6640",
      "failedColor": "#d96462B0",
      "failedEndColor": "#d9646240",
      "passedColor": "#63994dB0",
      "passedEndColor": "#63994d40"
    },

    "Slider": {
      "background": "$panelBackground",
      "tickColor": "$mutedForeground"
    },

    "Counter": {
      "background": "#82828Fb2",
      "foreground": "$panelBackground"
    },

    "DragAndDrop": {
      "areaBackground": "#CCDFFF",
      "areaBorderColor": "#CCDFFF",
      "areaForeground": "$regularForeground"
    },

    "Editor": {
      "background": "$textInputBackground",
      "foreground": "$regularForeground",
      "shortcutForeground": "$linkRegular"
    },

    "FileColor": {
      "Rose": "#ffe6e9",
      "Orange": "#ffefe6",
      "Yellow": "#f7f0d2",
      "Green": "#e1eddc",
      "Blue": "#dae4f7",
      "Violet": "#f1e6ff"
    },

    "InplaceRefactoringPopup": {
      "background": "$panelBackground",
      "borderColor": "$border"
    },

    "TabbedPane": {
      "background": "$panelBackground",
      "underlineColor": "#5397e6",
      "disabledUnderlineColor": "#CCCCCC",
      "contentAreaColor": "$border",
      "hoverColor": "#D0D0D0"
    },

    "VersionControl": {

    },

    "WelcomeScreen": {
      "background": "#F0F0F0",
      "borderColor": "#FF0000",
      "separatorColor": "$separator",
      "captionBackground": "#0000FF",
      "captionForeground": "#FFFF00",
      "footerBackground": "#FF00FF",
      "footerForeground": "#00FFFF",
      "headerBackground": "#800000",
      "headerForeground": "#008000",
      "groupIconBorderColor": "#000080",
      "Projects": {
        "background": "$toolwindowBackground",
        "selectionBackground": "$selectionBackground",
        "selectionInactiveBackground": "$selectionBackgroundInactive"
      }
    },

    "Group": {
      "separatorColor": "$separator"
    },

    "TitlePane": {
      "background": "#FFFFFF",
      "inactiveBackground": "$panelBackground",
      "infoForeground": "$mutedForeground",
      "inactiveInfoForeground": "$disabledForeground"
    },

    "ScrollPane": {
      "background": "$panelBackground",
      "foreground": "$regularForeground"
    },

    "EditorPane": {
      "inactiveBackground": "$panelBackground",
      "foreground": "$regularForeground"
    },

    "DebuggerTabs.selectedBackground": "#FFFFFF",

    "Window.border": "0,0,0,0,FFFFFF33",
    "DebuggerPopup.borderColor": "$popupBorder",
    "Separator.separatorColor": "$separator",
    "SidePanel.background": "$toolwindowBackground",
    "OptionPane.background": "$panelBackground"
  },

  "colors": {
    "$selectionBackground": "#99C8FF",
    "$selectionBackgroundInactive": "#c8cacc",
    "$selectionForeground": "#000000",
    "$selectionForegroundInactive": "#000000",
    "$border": "#D0D0D0",
    "$popupBorder": "#B0B0B0",
    "$controlBorder": "#D0D0D0",
    "$focusedBorder": "#83acdb",
    "$separator": "#D0D0D0",
    "$toolwindowBackground": "#FBFBFB",
    "$panelBackground": "#E8E8E8",
    "$regularForeground": "#333338",
    "$mutedForeground": "#767680",
    "$disabledForeground": "#a7a7af",
    "$textInputBackground": "#FFFFFF",
    "$linkRegular": "#5c85cc",
    "$linkAccented": "#1295DB",
    "$buttonFace": "#F8F8F8",
    "$notificationBackground": "#f2f7ff",
    "$notificationBorder": "#f7ffff",
    "$notificationForeground": "#2c3f61",
    "$validationErrorBackground": "#FFE3E3",
    "$validationErrorBorder": "#FFEDED",
    "$validationErrorForeground": "#B5494F",
    "$validationWarningBackground": "#FAEED5",
    "$validationWarningBorder": "#FFF7E6",
    "$validationWarningForeground": "#A87300",
    "$validationInformativeBackground": "#DCF0E1",
    "$validationInformativeBorder": "#E6F5E9",
    "$validationInformativeForeground": "#378C49"
  },

  "icons": {
    "/nodes/folder.svg": "/any/folder.svg",
    "/nodes/treeRightArrow.png": "/light/treeRightArrow.svg",
    "/nodes/treeDownArrow.png": "/light/treeDownArrow.svg",
    "/mac/tree_black_right_arrow.png": "/light/tree_black_right_arrow.svg",
    "/mac/tree_white_right_arrow.png": "/light/tree_white_right_arrow.svg",
    "/mac/tree_white_down_arrow.png": "/light/tree_white_down_arrow.svg",
    "ColorPalette": {
      "Actions.Grey": "#82828fFF",
      "Actions.GreyInline": "#82828eFF",
      "Actions.GreyInline.Dark": "#b8b8bfFF",
      "Actions.Red": "#e87669FF",
      "Actions.Green": "#6ba653FF",
      "Actions.Blue": "#5f8ad4FF",
      "Actions.Yellow": "#dba716FF",

      "Objects.Grey": "#82828dFF",
      "Objects.BlackText": "#383848FF",
      "Objects.Red": "#e87669FF",
      "Objects.RedStatus": "#e87669FF",
      "Objects.Yellow": "#dba716FF",
      "Objects.YellowDark": "#dba716FF",
      "Objects.Green": "#6ba653FF",
      "Objects.GreenAndroid": "#6ba653FF",
      "Objects.Blue": "#5f8ad4FF",
      "Objects.Purple": "#b36ab1FF",
      "Objects.Pink": "#e8699cFF",

      "Checkbox.Background.Default": "#ff00ff"
    }
  }
}